<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.solaceisle.mapper.SafeSpaceMapper">


    <select id="page" resultType="com.solaceisle.pojo.vo.SafeSpacePageVO">
        select * from safe_space
        <if test="order==1">
            order by create_time desc
        </if>
        <if test="order==2">
            order by like_count desc
        </if>
    </select>
</mapper>
